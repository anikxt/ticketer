{
  "manifest_version": 3,
  "name": "Ticketer",
  "version": "1.0",
  "description": "Ticketer is a tool that helps you report bugs or request features",
  "background": {
    "service_worker": "service-worker.js"
  },
  "permissions": ["sidePanel", "tabs", "identity", "scripting", "activeTab"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_title": "Open Ticketer",
    "default_icon": {
      "16": "assets/images/icon-16.png",
      "32": "assets/images/icon-32.png",
      "48": "assets/images/icon-48.png",
      "128": "assets/images/icon-128.png"
    }
  },
  "oauth2": {
    "client_id": "735854871768-ojaiucoce27v1ob8g6s83ajmrq040k4l.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/gmail.send",
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  },
  "web_accessible_resources": [
    {
      "resources": ["assets/libs/pdfjs-worker.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "16": "assets/images/icon-16.png",
    "32": "assets/images/icon-32.png",
    "48": "assets/images/icon-48.png",
    "128": "assets/images/icon-128.png"
  }
}
